<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="LaTeXView">
        <attr name="latexText" format="string"/>
        <attr name="size" format="float"/>
        <attr name="style" format="enum">
            <enum name="display" value="0"/>
            <enum name="text" value="2"/>
            <enum name="script" value="4"/>
            <enum name="script_script" value="6"/>
        </attr>
        <attr name="type">
            <flag name="serif" value="0"/>
            <flag name="sansserif" value="1"/>
            <flag name="bold" value="2"/>
            <flag name="italic" value="4"/>
            <flag name="roman" value="8"/>
            <flag name="typewriter" value="16"/>
        </attr>
        <attr name="foregroundColor" format="color"/>
        <attr name="backgroundColor" format="color"/>
    </declare-styleable>
</resources>